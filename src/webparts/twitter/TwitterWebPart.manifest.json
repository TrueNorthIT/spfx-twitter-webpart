  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", // Other
    "group": { "default": "Other" },
    "title": { "default": "Twitter" },
    "description": { "default": "Twitter Feed" },
    // "officeFabricIconFontName": "Page",
    "iconImageUrl" : "data:image/png;base64,",
    "properties": {
      "screenname": "TrueNorthIT",
      "autoLimit": true,
      "limit": 250,
      "header": true,
      "footer": false,
      "borders": false,
      "scrollbars": true,
      "transparent": true,
      "linkColor": "#820bbb",
      "borderColor": "#a80000"
    }
  }]
}
